<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Kjeld Ingemann Poulsen</Authors>
    <Company>Kipon ApS</Company>
    <Description>Library to stream .NET data to and from Excel (OpenXml)</Description>
    <Copyright>Kipon ApS 2019,2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/kip-dk/Kipon.Excel</PackageProjectUrl>
    <PackageIconUrl>https://s.gravatar.com/avatar/48d1270fbf870a670d20844eda19e3bf</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/kip-dk/Kipon.Excel</RepositoryUrl>
    <PackageTags>OpenXml Excel .NET Stream</PackageTags>
    <PackageReleaseNotes>Api.ExcelStream added as a beginning to add a context, where more customizations from outside can be injected.</PackageReleaseNotes>
    <Version>1.0.18-alpha</Version>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Kipon.snk</AssemblyOriginatorKeyFile>
    <AssemblyVersion>1.0.19.0</AssemblyVersion>
    <FileVersion>1.0.19.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.9.1" />
  </ItemGroup>

</Project>
